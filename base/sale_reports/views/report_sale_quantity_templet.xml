<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data>
<template id="report_by_quantity_temp">
    <t t-call="web.html_container">
       
        <t t-call="web.basic_layout">

            <div class="header" style="border-bottom: 2px solid black">
                <h3 class="text-center">Report For: <span t-esc="partner_id[1]"/></h3>
                <h4 class="text-center">
                    <strong>From :</strong> <span t-esc="st_date"/>
                    <strong>To :</strong> <span t-esc="end_date"/>                   
                </h4>
            </div>
            
             <br></br>
                     <div class="article">
                      <table class="table table-bordered" dir="rtl"> 
                          <thead>
				<tr>
		                          <th class="text-center" style = "width: 20%">Date</th>
					  <th class="text-center" style = "width: 20%">Describtion</th>
					  <th class="text-center" style = "width: 20%">prouduct</th>
					 <th class="text-center" style = "width: 20%">Quantity</th>
					 <th class="text-center" style = "width: 20%">Delivered</th>
					 <th class="text-center" style = "width: 20%">Subtotal</th>
			         </tr>

			</thead>
                       <tbody>
                            <t t-foreach="docs" t-as="inv">
                                <tr>
                               	       
                                	       <td><t t-esc="inv.order_id.confirmation_date"/></td>
                                               <td><t t-esc="inv.order_id.name"/></td>
                                               <td><t t-esc="inv.product_id.name"/></td>
						<td><t t-esc="inv.product_uom_qty"/></td>
						<td><t t-esc="inv.qty_delivered"/></td>
						<td><t t-esc="inv.price_subtotal"/></td>                    	
			          </tr>
                            </t>
		       </tbody>
		       
                     </table>
           
                        
                     
                    
</div>

</t>

</t>

</template>
</data>
</odoo>
